{
  "name": "apppos-desktop",
  "version": "2.2.18",
  "description": "AppPOS - Syst√®me de Point de Vente",
  "main": "main.js",
  "private": false,
  "author": "5sensprod",
  "scripts": {
    "dev": "vite",
    "start": "electron .",
    "dev:electron": "concurrently \"vite\" \"cross-env NODE_ENV=development electron .\"",
    "dev:electron-only": "concurrently \"vite\" \"cross-env NODE_ENV=development electron .\"",
    "build": "vite build",
    "build:prod": "vite build && electron-builder --config build-full.json",
    "build:update": "vite build && cross-env SKIP_NODE_MODULES=true electron-builder --config build-update.json",
    "preview": "vite preview",
    "version-patch": "npm version patch",
    "version-minor": "npm version minor",
    "version-major": "npm version major",
    "release": "npm version patch && git push origin main --follow-tags && npm run publish",
    "publish": "cross-env-shell \"GH_TOKEN=$GH_TOKEN\" vite build && electron-builder --config build-full.json --publish always",
    "publish:update": "cross-env-shell \"GH_TOKEN=$GH_TOKEN\" vite build && cross-env SKIP_NODE_MODULES=true electron-builder --config build-update.json --publish always"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "bonjour": "^3.5.0",
    "cors": "^2.8.5",
    "electron-log": "^5.3.1",
    "electron-updater": "^6.3.9",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.3",
    "jsbarcode": "^3.11.6",
    "lucide-react": "^0.477.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^7.2.0",
    "react-select": "^5.10.1",
    "recharts": "^3.0.0",
    "selfsigned": "^2.4.1",
    "yup": "^1.6.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "electron": "29.0.0",
    "electron-builder": "^24.6.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "vite": "^5.0.0"
  }
}
